/** Callback request dialog style */
#block-callback {
    background-color: #FFF;
    background-image: none;
    margin: 0;
    width: 204px;
}

#block-callback .block-title {
    height: 88px;
    margin-left: 12px;
    text-align: left;
    width: 300px;
}

#block-callback .block-title strong span {
    font-size: 14px;
}

#block-callback .block-title strong:after {
    content:"Оставьте номер вашего телефона и наш оператор перезвонит вам в течение часа.";
    color: #000000;
    display: block;
    font-family: Arial;
    font-size: 12px;
    font-style: normal;
    font-variant: normal;
    font-weight: normal;
    line-height: 16px;
    margin: -10px 0 25px 0;
    width: 190px;
}

#block-callback .block-title span {
    color: #000000;
    line-height: 48px;
    font-size: 12px;
    font-weight: bold;
}

#block-callback ul.form-list {
    margin-top: -25px;
}

#block-callback ul.form-list>li {
    clear: both;
    display: block;
    margin-top: 12px;
    overflow: hidden;
    width:200px;
}

#block-callback ul.form-list>li:first-child + li + li {
    display: none;
}

#block-callback div.field {
    overflow: hidden;
}

#block-callback input[type=text], #block-callback textarea{
    height: 19px;
    margin: 0;
    resize: none;
    vertical-align: middle;
    width: 188px;
}

#block-callback input#call_telephone {
    float: right;
    margin-right: 11px;
    width: 160px;
}

#block-callback  ul.form-list>li:first-child + li .input-box:before {
    content: "+38";
    color: #000000;
    float: left;
    font-family: Arial;
    height: 17px;
    line-height: 25px;
}

#block-callback textarea {
    height: 50px;
}

#block-callback ul.form-list>li label {
    margin: 0;
    display: block;
    font-size: 12px;
    text-align: left;
    width: 200px;
}

#block-callback  ul.form-list>li:first-child + li label:first-child:after {
    content: "(10 цифр подряд)";
    font-style: italic;
}

#block-callback ul.form-list>li label.error-message {
    color: #D40707;
    display: none;
    width: 220px;
}

#block-callback  ul.form-list>li:first-child + li label.error-message{
