* {padding: 0;
margin: 0;
font-weight: normal;
outline: 0 none;}
html {min-width: 1024px;}
body {background: #fff;
font-family: arial, sans-serif;
font-size: 14px;
line-height: 20px;
font-weight: normal;
color: #000;
text-align: center;}
a {text-decoration: none;}
a:hover {text-decoration: underline;}
a, h1, h2, h3, h4, .block .block-title {color: #01227e;}
h1 {margin-top: 9px;margin-bottom: 22px;font-size: 28px;line-height: 30px;}
h2 {margin: 40px 0 18px 0;
font-size: 25px;
line-height: 26px;}
h3 {margin: 32px 0 14px 0;
font-size: 20px;}
h4, .block .block-title {margin-bottom: 10px;
font-size: 17px;}
fieldset, img {border: 0 none !important;}
p strong {font-weight: bold;}
hr {clear: both;
height: 1px;
margin: 40px 0;
border: 0 none;
background-color: #ddd;
color: #ddd;}
/* layout */
#page {width: 1024px;
margin: 0 auto;
text-align: left;}
noscript {display: block;
margin: 30px 0;
background: red;}
noscript pre {padding: 10px;}

/* layout - header */
#header {overflow: hidden;
margin-top: 5px;}
#logo, #navi, #box-login {float: left;}
#logo, #navi {margin: 11px 23px 0 0;}
#logo {display: block;
width: 190px;
height: 150px;
margin-top: 0;
background: #fff url(../img/to.gif) 0px 0px no-repeat;}
#meta-navi {list-style-type: none;
width: 598px;
height: 37px;
margin: 0;
padding-top: 18px;
background: #fff url(../img/gfx.png) 0 -106px no-repeat;
text-align: right;}
#meta-navi li {display: inline;
padding: 0 8px 0 4px;
background: transparent url(../img/gfx.png) right -62px no-repeat;}
#meta-navi li a {font-size: 12px;
color: #666;}
#meta-navi .active a {color: #000;}
#meta-navi .last {background-image: none;}
#login {float: right;
width: 190px;}
#contacts {list-style-type: none;width: 586px;height: 40px;margin:2px 0 5px 0;}
#contacts li {width:136px;float:left;text-align:right;padding-right:10px;}
#contacts li span {font-weight:bold;padding-left:20px;margin-left:10px;display:block;height:40px;}
#contacts .contact.mts {background-position: 0 -303px;}
#contacts .contact.life {background-position: 0 -353px;}
#contacts .contact.ks {background-position: 0 -403px;}
#contacts .contact.landline {background-position: 0 -503px;}

/* layout - main */
#main {overflow: hidden;
margin: 5px 0 0 0;}
#col-content-left {overflow: hidden;
float: left;
width: 834px;}
#left, #content {margin-right: 23px;}
#content {float: right;
width: 598px;}
#content p {margin-bottom: 20px;}
.textblock em, .textblock strong {font-weight: bold;
font-style: normal;
color: #6f932e;}
#promotion {margin-bottom: 30px;}
#breadcrumbs {overflow: hidden;
list-style-type: none;
margin-bottom: 10px;}
#breadcrumbs li {float: left;
padding-right: 7px;}
#breadcrumbs li a, #breadcrumbs li span {padding-left: 10px;
background: transparent url(../img/gfx.png) 0 -229px no-repeat;
font-size: 12px;
color: #666;}
#breadcrumbs .first a, #breadcrumbs .first span {padding-left: 0;
background-image: none;}
#subcategory, .subcategories {overflow: hidden;
margin-bottom: 10px;
padding: 10px 0 10px 10px;
list-style-type: none;
background-color: #f6fee6;}
#subcategory li, .subcategories li{float: left;
width: 260px;
padding: 0 20px 0 10px;
background: transparent url(../img/gfx.png) -298px -227px no-repeat;}
#left {float: left;
width: 190px;}
#right {float: left;
width: 190px;}

/* classes */
.b, b {font-weight: bold;}
.tl {text-align: left;}
.tr {text-align: right;}
.tc {text-align: center;}
.fl {float: left;}
.fr {float: right;}
.cl {clear: left;}
.rnd {border-radius: 5px;
-opera-border-radius: 5px;
-moz-border-radius: 5px;
-webkit-border-radius: 5px;}
.navi {clear: both;
overflow: hidden;
display: block;
width: 578px;
padding: 0 10px;
list-style-type: none;
background: #0462B9;
background: #0462B9 url(../img/gradients.png) 0 0 repeat-x;
line-height: 34px;}
.navi li {float: left;
padding: 0 12px 0 10px;
background: transparent url(../img/gfx.png) right top no-repeat;}
.navi li a {display: block;
font-size: 15px;
font-weight: bold;
color: #fff;
text-shadow: #01529e 1px 1px 0;}
.navi .active a {color: #4d6e14;
text-shadow: none;}
.navi .last {background-image: none;
padding-right: 0;}
.block {overflow: visible;
width: 168px;
margin-bottom: 10px;
padding: 10px 10px;
border: 1px solid #d1d1d1;
border-radius: 5px;
-opera-border-radius: 5px;
-moz-border-radius: 5px;
-webkit-border-radius: 5px;
background: #fff url(../img/gradients.png) 0 -702px repeat-x;}
.block p {margin-bottom: 5px;}
.block-green {width: 170px;
border: 0 none;
background: #3EA0F6 url(../img/gradients.png) 0 -44px repeat-x;
color: #fff;}
.block-green label:hover {color: #eee;}
.block-green h4, .block-green .block-title, .block-green a, .block-green p, .block-green label {color: #fff;
text-shadow: #0563ba 1px 1px 1px;}
.block-list {width: 160px;
/*border: 0 none;*/
background: #ffffff none;}
.hint {margin: 5px 0;
font-size: 12px;
line-height: 14px;}
.ti {height: 20px;
line-height: 20px;
margin-bottom: 3px;}
.ti, .ta {padding: 0 2px;
border: 1px solid #aaa;
box-shadow: 1px 1px 3px #999 inset;
-moz-box-shadow: 1px 1px 3px #999 inset;
-webkit-box-shadow: 1px 1px 3px #999 inset;}
.ti:focus, .ta:focus {border-color: #666;}
.ti-wide, .ta-wide {width: 300px;}
.ti-last {margin-right: 0px !important;}
.btn {height: 21px;
padding: 0 5px 0 7px;
background: #0462B9 url(../img/gradients.png) 0 -333px repeat-x;
border: 0 none;
border-radius: 5px;
-opera-border-radius: 5px;
-moz-border-radius: 5px;
-webkit-border-radius: 5px;
cursor: pointer;
vertical-align: middle;
text-shadow: #555 1px 1px 1px;}
.btn:hover {background-color: #0563BA;
background-position: 0 -382px;}
.btn:active {background-color: #3EA0F6;
background-position: 0 -431px;}
#left .btn, #right .btn, #header .btn {box-shadow: 1px 1px 2px #222;
-moz-box-shadow: 1px 1px 2px #222;
-webkit-box-shadow: 1px 1px 2px #222;}
.btn span {padding-right: 10px;
background: transparent url(../img/gfx2.png?1) right -61px no-repeat;
font-weight: bold;
line-height: 21px;
color: #fff;}
.btngrey {background: #8b8c8c url(../img/gradients.png) 0 -480px repeat-x;
text-shadow: #999 1px 1px 1px;}
.btngrey:hover {background-color: #7f7f7f;
background-position: 0 -529px;}
.btngrey:active {background-color: #b2b2b2;
background-position: 0 -578px;}
.btnback {padding: 0 7px 0 5px;}
.btnback span {padding-right: 0;
padding-left: 10px;
background-position: 0 -706px;}
.btnhigh {height: 32px;}
.btninline {padding: 3px 5px 4px 7px;}
.btninline:hover {text-decoration: none;}
#left .btngrey, #right .btngrey {box-shadow: 1px 1px 3px #999;
-moz-box-shadow: 1px 1px 3px #999;
-webkit-box-shadow: 1px 1px 3px #999;}
.linklist {list-style-type: none;
line-height: 15px;}
.linklist li {margin: 5px 0;}
.link {padding-right: 10px;
background: transparent url(../img/gfx2.png?1) right -87px no-repeat;
font-size: 13px;
font-weight: bold;
color: #5a7d1c;}
.link-back {padding-right: 0;
padding-left: 10px;
background: transparent url(../img/gfx2.png?1) 0 -685px no-repeat;}
.block-green .link {background: transparent url(../img/gfx2.png?1) right -61px no-repeat;
color: #fff;}
.link-current {background-image: none;
color: #000;}
.bottom-links {margin-top: 20px;}
.ol {margin-left: 20px;}
.contentlist {list-style-type: circle;
margin-left: 20px;}
.contentlist li {line-height: 16px;
margin: 5px 0;}
.pagination {clear: both;
overflow: hidden;
padding: 5px 3px 3px;
background-color: #f9f9f9;}
.pagination p {margin: 5px 7px !important;}
.pagination .hint, .pagination .sort-by select {font-size: 13px;
color: #666;}
.amount {float: left;
text-align: left;}
.pages, .limiter {float: right;
text-align: right;}
.amount {font-size: 13px;
line-height: 20px;
color: #666;}
.sort-dir {display: block;
width: 18px;
height: 18px;
margin-top: 1px;
background: transparent url(../img/gfx.png) 0 0 no-repeat;}
.desc {background-position: -130px -276px;}
.desc:hover {background-position: -150px -276px;}
.desc:active {background-position: -170px -276px;}
.asc {background-position: -70px -276px;}
.asc:hover {background-position: -90px -276px;}
.asc:active {background-position: -110px -276px;}
.pages a, .pages span, .pages strong {padding: 5px 4px;
text-align: center;}
.pages a {color: #666;}
.pages a:hover {color: #000;}
.pages .btn {margin: 0 3px;
padding: 0 5px;
font-size: 16px;
font-weight: bold;
color: #fff;
text-decoration: none;}
.pages .btn:hover {color: #fff;}
.pages span {font-size: 12px;}
.redbox {width: 50%;
padding: 10px 10px 0 10px;
border: 2px solid red;}

/* grid products */
.product-grid {overflow: hidden;
padding: 20px 0;}
.p {position: relative;
display: block;
float: left;
width: 168px;
margin: 0 15px 20px 0;}
.p:hover {text-decoration: none;}
.p img {width: 110px;
height: 110px;
margin-right: 50px;}
.p-info {display: block;
padding: 3px 4px 5px 4px;
background-color: #f0f0f0;}
.p-contents {font-size: 13px;
line-height: 15px;}
.p:hover .p-info {background-color: #3EA0F6;}
.p:hover strong {color: #000;
text-shadow: #bbff39 1px 1px 0;}
.p:hover .price {color: #fff;
text-shadow: #688e20 1px 1px 0;}
.p:hover .p-msrp-saved {text-shadow: #caef85 1px 1px 0;}
.p:hover .p-contents {color: #333;
text-shadow: #bbff39 1px 1px 0;}
.p-first {clear: left;}
.p-last {margin-right: 0;}
.p strong {display: block;
padding: 15px 0 5px 0;
line-height: 15px;
color: #444;}
.p-contents {display: block;
color: #666;}
.p-info .price {display: block;
padding: 5px 0;
font-size: 20px;
font-weight: bold;
color: #eb337d;}
.p-msrp-saved {display: block;
padding-bottom: 3px;
font-size: 12px;
line-height: 13px;
color: #333;}
.p-msrp-value {text-decoration: line-through;}
.p-saved-amount {font-weight: bold;}

.p-grid-small .p {width: 120px;
margin-bottom: 30px;}
.p-grid-small .p img {width: 70px;
height: 70px;
margin: 0 40px 0 10px;}
.p-grid-small .p-info {padding: 5px 7px;}
.p-grid-small strong {padding: 0;
font-size: 13px;}
.d-box {position: absolute;
overflow: hidden;
top: 75px;
left: 33px;
width: 130px;
height: 50px;
height: 50px;
background: transparent url(../img/gfx2.png?1) 0 -620px no-repeat;
color: #fff;
text-shadow: #999 1px 1px 1px;}
.d-value, .d-label {position: absolute;}
.d-value {top: 18px;
left: 2px;
width: 125px;
text-align: center;
font-size: 26px;
font-weight: bold;
line-height: 24px;}
.d-value span {padding: 0 3px;
font-weight: bold;}
.d-label {top: 25px;
left: 70px;
font-size: 12px;}
.d-big {background: transparent url(../img/gfx2.png?1) 0 -568px no-repeat;}
.d-tip .d-label {width: 125px;
top: 20px;
left: 2px;
font-size: 18px;
font-weight: bold;
text-align: center;}

/* category view */
.c-description {margin: 30px 0;}
.category-header .category_image {float:left;margin:0 10px 10px 0;}

#my-car-block.my-car-block-tires {
    height: 148px;
}
.tires-vehicle-filter {
    display: block;
    margin-top: 12px;
    padding: 0;
}
ul.tires-vehicle-filter li {
    display: block;
    margin-right: 5px;
    list-style-type: none;
    width: 150px;
}
.tires-search select{
    float: none;
    max-width: 100px;
    width: 100px!important;
    vertical-align: middle;
}
.tires-by-avto{
    margin-bottom: 10px;}

/* cart */
.cart-wrap {margin-bottom: 30px;
background-color: #e9f4d5;}
.cart-wrap p {padding: 0 26px;}
.cart-wrap .btn {width: 250px;}
.cart-table, .used-in-cars-table, .tbl {border-collapse: separate;
border-spacing: 1px;
width: 100%;}
.cart-table th, .cart-table td, .used-in-cars-table th, .used-in-cars-table td, .tbl th, .tbl td{padding: 4px 8px;}
.cart-table tbody td, .tbl tbody td {background-color: #fff;}
.cart-table tbody tr:hover td, .tbl tbody tr:hover td {background-color: #f6f6f6;}
.cart-table tfoot td {padding: 25px 25px 25px 10px;
vertical-align: top;}
.cart-table tfoot .tl {/*padding-top: 62px;*/
padding-right: 10px;}
.cart-table .ti {width: 40px;
margin: 1px 0 0 0;
text-align: center;}
.cart-table .cart-totals {padding: 25px 10px;}
.cart-table .cart-item-name {width: 220px;}
.cart-table .del {width: 55px;}
.btn-del {display: block;
width: 20px;
height: 20px;
margin: 0 auto;
background: #850000 url(../img/gfx.png) -1px -279px no-repeat;}
.btn-del:hover {background-position: -23px -279px;}
.btn-del:active {background-position: -45px -279px;}
.cart-table dl {width: 100%;
margin: 0;
overflow: hidden;}
.cart-table dl dt {float: left;
width: 50%;
text-align: left;}
.cart-table dl dd {float: right;
width: 50%;
text-align: right;}
.cart-table dl .last, .cart-table dl .last .price {font-size: 20px;
font-weight: bold;
line-height: 30px;
color: #5a7d1c;}
.checkout-cart-index .cart-totals {padding: 25px;}
.checkout-cart-index #cart-table-totals {float: right;
width: 230px;
margin: 15px 10px;}
.shipping_promotion {list-style-type: none;
margin-bottom: 12px;}
.shipping_promotion li {padding: 7px 0 3px 0;}
.shipping_promotion li {font-size: 12px;
line-height: 15px;
color: #666;}
.shipping_diff .price {font-weight: bold;}

/* random list */
.random-list {margin-top: 10px;
list-style-type: none;}
.random-list li {display: none;}
.random-list img {margin-top: 10px;}
.random-list a span {font-size: 13px;
font-weight: bold;}
.random-list a:hover {text-decoration: none;}
.random-list a:hover span {text-decoration: underline;}

/* blocks */
#block-search label {display: block;
width: 160px;
padding-bottom: 3px;}
#search {width: 110px;
margin-right: 3px;
float: left;}
#block-search .btn {}
#block-search #search_inside {clear: both; float: left; margin: 2px 2px 0 0;}
#search_autocomplete {width: 158px !important;
margin-top: 5px;
border: 1px solid #aaa;
-webkit-border-radius: 5px;
-webkit-border-top-left-radius: 0;
-moz-border-radius: 5px;
-moz-border-radius-topleft: 0;
border-radius: 5px;
border-top-left-radius: 0;
-moz-box-shadow: 3px 3px 4px #666;
-webkit-box-shadow: 3px 3px 4px #666;
box-shadow: 3px 3px 4px #666;
background: #fff url(../img/gradients.png) 0 -702px repeat-x;}
#search_autocomplete ul {list-style-type: none;}
#search_autocomplete ul li {padding: 0 5px;
color: #333;
cursor: pointer;}
#search_autocomplete ul li span {width: 30px;}
#search_autocomplete ul li:hover {background-color: #e9f4d5;
color: #000;}
#search_autocomplete ul li:hover span {text-decoration: none;}
#block-login .inputs {margin: 7px 0 4px 0;
padding-left: 50px;
background: transparent url(../img/gfx2.png?1) 0 0 no-repeat;}
#block-login {min-height: 127px;}
#block-login .block-title{margin-bottom: 7px;}
#block-login .ti {width: 105px}
#link-create-account, #link-lost-pw {float: left;}
#link-create-account {background-position: right -59px;
font-size: 13px;
font-weight: bold;}
#link-lost-pw {clear: both;
display: block;
width: 150px;
margin-top: 5px;
font-size: 12px;}
#block-login .btn {float: right;}
#block-apotheke {margin-bottom: 3px;}
#block-cart .cartinfo {margin: 10px 0 0 0;
padding: 10px 0 20px 70px;
background: transparent url(../img/gfx2.png?1) 0 -140px no-repeat;}
#block-cart .empty {background-position: 0 -235px;}
#block-cart .price {font-size: 16px;
font-weight: bold;}
#block-cart .btn {width: 158px;
margin-bottom: 3px;
padding: 0 3px 0 5px;}
#block-cart .wishlistlink {margin: 15px 0 8px 0;}
#block-search .block-title, #block-autoparts h4 {margin-bottom: 0;}
#block-subscribe .subscrinfo {padding: 0 0 0 70px;
background: transparent url(../img/gfx2.png?1) 0 -317px no-repeat;
font-size: 13px;
line-height: 16px;}
#block-poll .btn {margin-top: 10px;}
#block-twitter p {padding: 30px 0 0 0;
background: transparent url(../img/gfx2.png?1) 0 -420px no-repeat;}
#block-categories {width: 190px;
padding: 10px 0;}
#categories, #categories li ul {list-style-type: none;}
#categories li {margin: 7px 15px;
padding-left: 14px;
background: transparent url(../img/gfx.png) -300px -230px no-repeat;
font-size: 13px;
line-height: 16px;}
#categories li ul {margin: 5px -15px 8px -29px;
padding: 2px 0;
background-color: #f6fee6;}
#categories li ul li {margin: 4px 0;
padding: 0 10px 0 40px;
background: transparent none;}
#categories li ul li ul {margin: 5px 0px 3px -10px;}
#categories li ul li ul li{padding: 0 10px 0 20px;}
#categories .active > a {color: #3a4d18;}
.info-table {border-collapse: collapse;
border-spacing: 0;}
.info-table td, #block-popular-products p {vertical-align: top;
padding-bottom: 5px;
padding-right: 3px;
font-size: 12px;
line-height: 14px;
color: #6f932e;}
#block-popular-products a {display: block;
padding: 0 8px 0 0;}
#block-partners {width: 173px;
padding-right: 0;}
#block-partners p {font-size: 12px;}
#block-partners .partnerlogo {padding-left: 60px;
background: transparent url(../img/gfx2.png?1) 0 -490px no-repeat;}
#block-recommendation .product-grid {padding-bottom: 0;}
#block-recommendation .p {width: 158px;
margin-bottom: 0;
padding-top: 20px;
border-top: 1px solid #D1D1D1;
text-align: center;}
#block-recommendation .p:first-child {border-top: 0 none !important;}
#block-recommendation .p strong {padding-top: 0;}
#block-recommendation .p img {margin: 0 auto;}
#block-recommendation .p-info {background-color: transparent !important;}
#block-recommendation .p:hover strong {text-shadow: none;
text-decoration: underline;}
#block-recommendation .p:hover .price {color: #000;
text-shadow: none;}
/* product view */
.clearflow, .p-infos, .p-quantity, .p-price-cart, .p-meta {overflow: hidden;}
.p-infos img {float: left;
margin: 0 20px 0 0;}
.p-hint {font-size: 12px;
font-style: italic;
line-height: 14px;
color: #999;}
.p-buy {margin: 20px 0;}
.p-quantity strong {font-size: 16px;}
.p-quantity span {padding-right: 10px;}
.p-price {overflow: hidden; padding: 0 0 25px 0; width: 220px; height: 45px;}
.p-price p {margin-bottom: 0 !important;}
.p-price .price {float: left;
font-size: 16px;
font-weight: bold;
line-height: 30px;
color: #f00;}
.p-price .d-box {position: relative;
top: -15px;
left: 0;
float: right;}
.p-qty {width: 65px;
height: 19px;
margin-right: 3px;
padding: 8px 0;
background: #7a7a7a url(../img/gradients.png) 0 -627px repeat-x;
font-size: 12px;
text-align: center;}
.p-qty input {width: 20px;
height: 18px;
margin: 0;
font-size: 12px;
line-height: 18px;
text-align: center;}
.add-to-cart {width: 225px;
padding-top: 10px;}
.add-to-cart button {padding: 0 8px 0 10px;
width: 156px;
height: 35px;}
.p-meta {clear: both;
margin: -5px 0 5px 0;
font-size: 13px;
color: #333;}
.p-meta em {font-style: normal;
color: #6f932e;}
.p-meta span {padding: 0 8px 0 6px;
background: transparent url(../img/gfx.png) right -62px no-repeat;}
.p-meta span:first-child, .p-meta .first {padding-left: 0;}
.p-meta span:last-child, .p-meta .last {padding-right: 0;
background-image: none;}
.p-collateral {margin-top: 20px;}
.p-collateral ul {list-style-type: circle;margin: 0 0 20px 20px;}
.p-collateral ol, .category_description ul {margin: 0 0 20px 20px;}
.p-collateral strong {font-weight: bold;}
.p-collateral ul.subcategories {margin: 0 0 10px 0; padding: 10px 0 10px 10px; list-style-type: none;}
.p-collateral ul.subcategories li{padding: 0 20px 0 10px;
margin: 0;}
/* forms */
label:hover {color: #222;}
.form fieldset {overflow: hidden;
margin-bottom: 10px;}
.form fieldset div {clear: both;
overflow: hidden;
margin-bottom: 10px;}
.form label {width: 172px;
margin-bottom: 3px;
padding-top: 3px;
line-height: 15px;
cursor: pointer;}
.form label:hover {text-decoration: underline;}
.form label, .form .ti {float: left;
margin-right: 20px;}
.form .wide label {display: block;
float: none;
width: 320px;
margin-bottom: 10px;}
.form label em {padding-left: 3px;
font-size: 18px;
font-weight: bold;
font-style: normal;
line-height: 15px;
color: #486600;}
.form .rb, .form .cb {float: left;
margin: 3px 5px 0 0;
cursor: pointer;}
.form .radiolabel {width: auto;}
.form select {border: 1px solid #aaa;}
